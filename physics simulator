import pygame, pymunk 

pygame.init()

SCREEN_WIDTH = 600 
SCREEN_HEIGHT = 600 

screen = pygame.display.set_mode((SCREEN_WIDTH, SCREEN_HEIGHT))


def create








running = True 
while running: 
    for event in pygame.event.get(): 
        if event.type == pygame.QUIT: 
            running = False
    
    screen.fill((217, 217, 217)) 
    pygame.display.update()